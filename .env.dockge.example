# Required: set admin token for /admin and /v1/devices*
TS_HUB_ADMIN_TOKEN=replace-with-strong-admin-token

# Required: set at least one of these two
TS_HUB_SIGNING_KEY=replace-with-long-random-signing-key
TS_HUB_INVITE_TOKEN=replace-with-invite-token

# Optional hardening tuning
TS_HUB_ADMIN_LOGIN_WINDOW_SEC=300
TS_HUB_ADMIN_LOGIN_MAX_ATTEMPTS=10
